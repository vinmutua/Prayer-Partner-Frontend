{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/prayer-partners-app",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
